	<link name="base_link">
        <visual>
            <geometry>
                <mesh filename="../meshes/body/body_edited_2.dae"
                      scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="black"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
